@article{McNamara,
  abstract   = {It is well known that if a finite graded lattice of rank n is supersolvable, then it has an EL-labeling where the labels along any maximal chain form a permutation. We call such a labeling an Sn EL-labeling and we show that a finite graded lattice of rank n is supersolvable if and only if it has such a labeling. We next consider finite graded posets of rank n with 0̂ and 1̂ that have an Sn EL-labeling. We describe a type A 0-Hecke algebra action on the maximal chains of such posets. This action is local and gives a representation of these Hecke algebras whose character has characteristic that is closely related to Ehrenborg's flag quasisymmetric function. We ask what other classes of posets have such an action and in particular we show that finite graded lattices of rank n have such an action if and only if they have an Sn EL-labeling.},
  author     = {Peter McNamara},
  doi        = {https://doi.org/10.1016/S0097-3165(02)00019-5},
  issn       = {0097-3165},
  journal    = {Journal of Combinatorial Theory, Series A},
  keywords   = {Poset, Lattice, EL-labeling, Supersolvable lattice, Hecke algebra action, Quasisymmetric function},
  number     = {1},
  pages      = {69-89},
  title      = {EL-labelings, supersolvability and 0-Hecke algebra actions on posets},
  url        = {https://www.sciencedirect.com/science/article/pii/S0097316502000195},
  volume     = {101},
  year       = {2003},
  bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0097316502000195},
  bdsk-url-2 = {https://doi.org/10.1016/S0097-3165(02)00019-5}
}

