@article{oum-seymour,
  title    = {Testing branch-width},
  journal  = {Journal of Combinatorial Theory, Series B},
  volume   = {97},
  number   = {3},
  pages    = {385-393},
  year     = {2007},
  issn     = {0095-8956},
  doi      = {https://doi.org/10.1016/j.jctb.2006.06.006},
  url      = {https://www.sciencedirect.com/science/article/pii/S0095895606000918},
  author   = {Sang-il Oum and Paul Seymour},
  keywords = {Branch-width, Tangle, Connectivity function, Symmetric submodular function, Rank-width, Carving-width},
  abstract = {An integer-valued function f on the set 2V of all subsets of a finite set V is a connectivity function if it satisfies the following conditions: (1) f(X)+f(Y)⩾f(X∩Y)+f(X∪Y) for all subsets X, Y of V, (2) f(X)=f(V∖X) for all X⊆V, and (3) f(∅)=0. Branch-width is defined for graphs, matroids, and more generally, connectivity functions. We show that for each constant k, there is a polynomial-time (in |V|) algorithm to decide whether the branch-width of a connectivity function f is at most k, if f is given by an oracle. This algorithm can be applied to branch-width, carving-width, and rank-width of graphs. In particular, we can recognize matroids M of branch-width at most k in polynomial (in |E(M)|) time if the matroid is given by an independence oracle.}
}

